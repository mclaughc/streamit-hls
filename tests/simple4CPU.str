int->int pipeline simple4CPU {
    add counter();
    //add stuff();
    add adder();
    add printer();
}

void->int filter counter {
    int last = 1;
    work push 1 {
        push(last);
        last = last + 1;
    }
}

int->int filter adder {
    work pop 1 push 1 {
        push(pop() * 2);
    }
}
int->int filter subber {
    work pop 1 push 1 {
        push(pop());
    }
}

int->int splitjoin stuff {
    split duplicate;
    add adder();
    add subber();
    join roundrobin;
}

int->int filter printer {
    work pop 1 {
        int x = pop();
        println(x);
    }
}

